-- Scripts:

local function WVWZO_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local PandaAuth = loadstring(game:HttpGet("https://raw.githubusercontent.com/Panda-Repositories/PandaKS_Libraries/main/library/LuaLib/ROBLOX/VAL", true))()
	
	local frame = script.Parent:WaitForChild("Frame")
	local getKeyButton = frame:WaitForChild("GetKeyButton")
	local keyTextBox = frame:WaitForChild("KeyTextBox")
	local checkKeyButton = frame:WaitForChild("CheckKeyButton")
	local statusLabel = frame:WaitForChild("StatusLabel")
	
	-- Deixe o statusLabel invisível por padrão
	statusLabel.Visible = false
	
	local InternalTable = {
		Service = "inifinityhub", -- Nome do serviço atualizado
		APIToken = "jB4DC6JTXS0kcxcl", -- Token da API atualizado
		VigenereKey = tostring({}) .. tostring(function() end) .. tostring(workspace["GetServerTimeNow" .. ("\0"):rep(math.random(2, 32))](workspace) + math.random(os.clock(), 999999999 or os.time())) .. ("\0"):rep(math.random(2, 32) % os.time() + os.clock()),
		TrueEndpoint = tostring({}) .. tostring(function() end) .. tostring(workspace["GetServerTimeNow" .. ("\0"):rep(math.random(2, 32))](workspace) + math.random(os.clock(), 999999999 or os.time())) .. ("\0"):rep(math.random(2, 32) % os.time() + os.clock()),
		FalseEndpoint = tostring({}) .. tostring(function() end) .. tostring(workspace["GetServerTimeNow" .. ("\0"):rep(math.random(2, 32))](workspace) + math.random(os.clock(), 999999999 or os.time())) .. ("\0"):rep(math.random(2, 32) % os.time() + os.clock()),
		Webhook = nil,
		Debug = false 
	}
	
	local Internal = setmetatable({}, {
		__index = function(self, key)
			return rawget(InternalTable, key)
		end;
		__newindex = function(self, key, value)
			while true do end; repeat until false; print(debug.traceback()) return
		end;
		__tostring = function(self)
			while true do end; repeat until false; print(debug.traceback()) return
		end;
	})
	
	PandaAuth:SetInternal(Internal)
	
	local function validateKey(key)
		local result = PandaAuth:ValidateKey(key)
	
		if result and result["Validated"] then
			return true, "Chave correta!"
		else
			return false, "Chave incorreta."
		end
	end
	
	-- Função para copiar texto para a área de transferência (apenas em ambiente local)
	local function copyToClipboard(text)
		if setclipboard then
			setclipboard(text)
		end
	end
	
	-- Ação ao clicar no botão de obter chave
	getKeyButton.MouseButton1Click:Connect(function()
		local newKey = "https://pandadevelopment.net/getkey?service=inifinityhub&hwid=011202e4-1993-4d13-97f8-0d6b7d899a51" -- Gere sua chave aqui
		copyToClipboard(newKey)
		statusLabel.Text = "Chave copiada para a área de transferência: " .. newKey
		statusLabel.TextColor3 = Color3.new(0, 1, 0) -- Verde para sucesso
		statusLabel.Visible = true
	end)
	
	-- Ação ao clicar no botão de verificar chave
	checkKeyButton.MouseButton1Click:Connect(function()
		local inputKey = keyTextBox.Text
		local isValid, message = validateKey(inputKey)
		statusLabel.Text = message
		if isValid then
			statusLabel.TextColor3 = Color3.new(0, 1, 0) -- Verde para sucesso
		else
			statusLabel.TextColor3 = Color3.new(1, 0, 0) -- Vermelho para erro
		end
		statusLabel.Visible = true
	end)
	
end
coroutine.wrap(WVWZO_fake_script)()
